<?xml version="1.0" encoding="UTF-8"?>
<aiml version="1.0">

    <category>
        <pattern>SAY *</pattern>
        <template>
            [SAY]
            <srai>
                <star/>
            </srai>
        </template>
    </category>

    <category>
        <pattern>HELLO</pattern>
        <template>
            <srai>GREETINGS</srai>
        </template>
    </category>

    <category>
        <pattern>LOOKATME</pattern>
        <template>
            <random>
                <li>Hey<get name="Operator"/>, please pay attention!
                </li>
                <li><get name="Operator"/>, please focus!
                </li>
                <li>Do not distract,<get name="Operator"/>!
                </li>
            </random>
            <random>
                <li>Our help is needed!</li>
                <li>Someone requires our help!</li>
                <li>Duty calls!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>INSPECTME</pattern>
        <template>
            <random>
                <li>I'm ready for the inspection!</li>
                <li>Come on, try me!</li>
                <li>I'm ready to start the inspection!</li>
                <li>I'm ready! Inspect me!</li>
            </random>
            Push the button to continue..
        </template>
    </category>

    <category>
        <pattern>DOOROPEN</pattern>
        <template>
            <random>
                <li>Could you open the door, please?</li>
                <li>I'm waiting! Could you open the door?</li>
                <li>I'm ready! Please open the door!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>OBSTACLE</pattern>
        <template>
            <random>
                <li>Ops! I see an obstacle in front of me!</li>
                <li>I can't go on! There's an obstacle!</li>
                <li>I'm sorry, but I can't proceed. There's an obstacle!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>COMEHERE</pattern>
        <template>
            <random>
                <li>Could you please come here?</li>
                <li>Please, come here!</li>
                <li>Come here, please!</li>
                <li>Can you come here?</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>PERSONDETECTED</pattern>
        <template>
            <random>
                <li>I saw you!</li>
                <li>There you are!</li>
                <li>Oh, you are there!</li>
            </random>
            <random>
                <li>I'm getting there!</li>
                <li>Wait for me!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>COMEHERE CUSTOMER *</pattern>
        <template>
            <random>
                <li><star/>!
                </li>
                <li>Hey<star/>!
                </li>
                <li>Where is<star/>?
                </li>
            </random>
            <random>
                <li>Could you please come here?</li>
                <li>Please, come here!</li>
                <li>Come here, please!</li>
                <li>Can you come here?</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>ORDER CUSTOMER * DRINK *</pattern>
        <template>
            <random>
                <li>Hi barman!</li>
                <li>Hi!</li>
                <li>Cheers my friend!</li>
                <li>Hi buddy!</li>
            </random>
            <random>
                <li>I have an order for<star index="1"/>. A<star index="2"/>, please!
                </li>
                <li>
                    <star index="1"/>
                    ordered a<star index="2"/>.
                </li>
                <li>
                    <star index="1"/>
                    would like a<star index="2"/>!
                </li>
                <li>A
                    <star index="2"/>
                    for<star index="1"/>, please!
                </li>
            </random>
        </template>
    </category>

    <category>
        <pattern>CONFIRMDRINK CUSTOMER * DRINK *</pattern>
        <template>
            <random>
                <li>Nice!</li>
                <li>Alright!</li>
                <li>Good!</li>
                <li>Perfect!</li>
            </random>
            <random>
                <li>I've seen that the
                    <star index="2"/>
                    for
                    <star index="1"/>
                    is available.
                </li>
                <li>My sensors confirm that the
                    <star index="2"/>
                    for
                    <star index="1"/>
                    is available.
                </li>
            </random>
        </template>
    </category>

    <category>
        <pattern>CONFIRMNOTAVAILABLE CUSTOMER * DRINK *</pattern>
        <template>
            <random>
                <li>Oh I'm sorry!</li>
                <li>Mmm...</li>
            </random>
            <random>
                <li>I've seen that the
                    <star index="2"/>
                    for
                    <star index="1"/>
                    is not available.
                </li>
                <li>I'm pretty sure that the
                    <star index="2"/>
                    for
                    <star index="1"/>
                    is not available.
                </li>
                <li>I don't see any
                    <star index="2"/>
                    for
                    <star index="1"/>
                    here.
                </li>
            </random>
        </template>
    </category>

    <category>
        <pattern>SPRINFOCOLLECTED TOTAL * MALES * FEMALES *</pattern>
        <template>
            Scan completed!
            <random>
                <li>There are <star index="1"/> persons in the room, <star index="2"/> are males and <star index="3"/> are females!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>SPRINFOCOLLECTED DONOTKNOW</pattern>
        <template>
            Scan completed!
            <random>
                <li>I'm sorry, but I wasn't able to detect you!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>FOLLOWYOU</pattern>
        <template>
            <random>
                <li>Okay!</li>
                <li>Alright!</li>
                <li>Good!</li>
                <li>Perfect!</li>
            </random>
            <random>
                <li>I'm following you.</li>
                <li>I'm chasing you.</li>
            </random>
            Please go ahead!
            |[STOP]
        </template>
    </category>

    <category>
        <pattern>FOLLOWME</pattern>
        <template>
            Okay!
            <random>
                <li>Now, please follow me.</li>
                <li>Follow me.</li>
            </random>
            Stay behind me!
            |[STOP]
        </template>
    </category>

    <category>
        <pattern>STOPFOLLOWING</pattern>
        <template>
            <think>
                <set name="topic"></set>
            </think>
            <random>
                <li>Okay!</li>
                <li>Alright!</li>
                <li>Good!</li>
                <li>Perfect!</li>
            </random>
            <random>
                <li>Here we are.</li>
                <li>Here I am.</li>
            </random>|[STOPFOLLOWING]
        </template>
    </category>

    <category>
        <pattern>PLAYRIDDLES</pattern>
        <template>
            Hello everybody! Welcome to the SPR test.
            <random>
                <li>I want to play riddles!</li>
                <li>I'd like to play riddles!</li>
                <li>Let's play riddles!</li>
            </random>
            I'll wait for ten seconds!
        </template>
    </category>

    <category>
        <pattern>PLAYWITHME</pattern>
        <template>
            <random>
                <li>Who wants to play with me?</li>
                <li>Is there anyone that wants to play with me?</li>
                <li>I need a volunteer to play with me!</li>
            </random>
            Please, stay still and look at me! I'll let you know when we are ready!
        </template>
    </category>

    <category>
        <pattern>WEREADY</pattern>
        <template>
            <random>
                <li>I'm ready! What is you question?</li>
                <li>Come on, go ahead with you question!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>PLAYBLINDMAN</pattern>
        <template>
            <random>
                <li>Let's play the blind man's bluff game!</li>
            </random>
            I'll wait for ten seconds..
        </template>
    </category>

    <category>
        <pattern>WHATNOW</pattern>
        <template>
            <random>
                <li>What can we do now?</li>
                <li>How can I help you?</li>
                <li>Is there something I can do?</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>NEXTQUESTION</pattern>
        <template>
            <random>
                <li>Let's move to the next question.</li>
                <li>Sorry, I don't understand. Let's go on.</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>STARTING</pattern>
        <template>
            <random>
                <li>Let's start!</li>
                <li>I'm ready to do it!</li>
                <li>Great! Let's do it!</li>
            </random>
        </template>
    </category>

    <category>
        <pattern>FAREWELL</pattern>
        <template>
            <random>
                <li>It was a pleasure to help you!</li>
                <li>Well done!</li>
                <li>Mission accomplished!</li>
            </random>
            Bye bye!
        </template>
    </category>

</aiml>