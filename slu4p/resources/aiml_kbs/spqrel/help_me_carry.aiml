    <topic name="FOLLOWING">

        <category>
            <pattern>START</pattern>
            <template>[SAY] Tell me when you are ready to go!</template>
        </category>

        <category>
            <pattern>*</pattern>
            <template>
                <srai>SAY FOLLOWYOU</srai>
                |[STOP]
            </template>
        </category>

        <category>
            <pattern>STOP</pattern>
            <template>
                [STOP]
                <set name="topic">WAITFORSTOPFOLLOWING</set>
            </template>
        </category>

    </topic>

