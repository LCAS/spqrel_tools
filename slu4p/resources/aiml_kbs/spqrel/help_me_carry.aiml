    <topic name="GUIDE">

        <category>
            <pattern>RESET</pattern>
            <template>
                <think>
                    <set name="Operator">TOFILL</set>
                </think>
            </template>
        </category>

        <category>
            <pattern>START</pattern>
            <template>
                <condition>
                    <li name="Operator" value="TOFILL">
                        <srai>AskName</srai>
                    </li>
                    <li>
                        <srai>NAMEACQUIRED</srai>
                    </li>
                </condition>
            </template>
        </category>

        <category>
            <pattern>ASKNAME</pattern>
            <template>
                [SAY]
                <random>
                    <li>Hi there!</li>
                    <li>Hello!</li>
                    <li>Nice to meet you!</li>
                </random>
                <random>
                    <li>Please, can you tell me your name?</li>
                    <li>What is your name?</li>
                    <li>What's your name?</li>
                    <li>Can I ask your name?</li>
                </random>
            </template>
        </category>

